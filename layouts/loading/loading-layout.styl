#loading-layout
    position fixed
    z-index 100
    top 0
    left 0
    background-color var(--background)
    display flex
    flex-direction column
    justify-content center
    align-items center
    width 100%
    height 100vh

#ycc-loading-icon
    width 48px
    height 48px
    object-fit contain
    object-position center